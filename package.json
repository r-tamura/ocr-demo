{
  "name": "ocr-demo",
  "private": true,
  "author": "rtamura",
  "license": "ISC",
  "scripts": {
    "start": "npm-run-all --parallel watch:*",
    "build:js": "webpack",
    "build:js:production": "cross-env NODE_ENV=production webpack",
    "build:css": "postcss -c .postcssrc.json",
    "watch:js": "webpack -w",
    "watch:css": "postcss -w -c .postcssrc.json",
    "watch:server": "http-server public -p 8080 -c-1",
    "typecheck": "flow",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "test": "cross-env NODE_ENV=test ava",
    "cover": "nyc --reporter=lcov --reporter=text yarn test"
  },
  "dependencies": {
    "autoprefixer": "^6.6.0",
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "csswring": "^5.1.1",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "flow-bin": "^0.37.4",
    "http-server": "^0.9.0",
    "istanbul": "^0.4.5",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.0.0",
    "postcss": "^5.2.8",
    "postcss-apply": "^0.4.0",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.0.0",
    "postcss-nesting": "^2.3.1",
    "postcss-sorting": "^1.7.0",
    "uglify-js": "^2.7.5",
    "watch": "^1.0.1",
    "webpack-async-await": "^1.1.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "eslint-config-airbnb": "^14.0.0",
    "webpack": "beta"
  }
}
